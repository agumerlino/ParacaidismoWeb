                                <div class="custom-dropdown-content">
                                    <a asp-area="" asp-controller="Home" asp-action="Index">Arnes-Velas</a>
                                    <a asp-area="" asp-controller="Home" asp-action="Index">Accesorios</a>
                                    <a asp-area="" asp-controller="Home" asp-action="Index">Ropa</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="custom-dropdown">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Productos" asp-action="Index"><span>PILOTO</span></a>
                                <div class="custom-dropdown-content">
                                    <a asp-area="" asp-controller="Home" asp-action="Index">Accesorios</a>
                                    <a asp-area="" asp-controller="Home" asp-action="Index">Ropa</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="custom-dropdown">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Productos" asp-action="Index"><span>OTROS</span></a>
                                <div class="custom-dropdown-content">
                                    <a asp-area="" asp-controller="Home" asp-action="Index">Camaras</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="custom-dropdown">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Productos" asp-action="Index"><span>QUIERO PUBLICAR MI PRODUCTO</span></a>
                                <div class="custom-dropdown-content">
                                </div>
                            </div>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    @* HEADER PARA DISPOSITIVO MOVIL *@
     <header class="d-md-none">
